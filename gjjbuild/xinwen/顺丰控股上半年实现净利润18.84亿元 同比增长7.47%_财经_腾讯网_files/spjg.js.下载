		})
	}

	function injectRocket(){
		var str=
		'<div class="gj-rtips">'+
			'<a class="gj-rclose">忽略提醒</a>'+
			'<a class="gj-rbtn"></a>'+
		'</div>'
		$('body').append(str)

		var tips=$('.gj-rtips')
		tips.find('.gj-rbtn').click(function(){
			report({tag:'news.omg.rocket.download'})
			openDownload()

		})
		tips.find('.gj-rclose').click(function(){
			tips.hide();
		})

		str=
		'<div id="gj-rwrap">'+
			'<div class="gj-rocket" state="gf1">'+

			'</div>'+
			'<div class="gj-rocketword"></div>'+
			'<div class="gj-smoke"></div>'+

		'</div>'
		$('body').append(str)
		

		report({tag:'news.omg.rocket.show'})

		var r=$('#gj-rwrap')
		var rocket=r.find('.gj-rocket')
		var smoke=r.find('.gj-smoke')
		var rw=r.find('.gj-rocketword')
		var jumpyInterval=0
		var orignalBottom=parseInt(rocket.css('bottom'))
		var smokeBottom=parseInt(smoke.css('bottom')),
		smokeRight=parseInt(smoke.css('right')),
		smokeWidth=smoke.width(),smokeHeight=smoke.height();

		rocket.bind('mouseenter',function(){
			// clearInterval(jumpyInterval)
			// jumpyInterval=setInterval(jumpyRocket,40)
			report({tag:'news.omg.rocket.hover'})
		})
		/*		rocket.bind('mouseleave',function(){
			// clearInterval(jumpyInterval)
			
			// rocket.attr('state','normal')
		})
		*/
		rocket.click(function(){
			openDownload()
			
			report({tag:'news.omg.rocket.launch'})

			clearInterval(jumpyInterval)

			var startBottom=orignalBottom

			var changeBottom=(startBottom+30)
			var endBottom=changeBottom+1024
			rocket.animate({
				bottom:changeBottom+'px'
			},60,function(){
				rocket.attr('state','gblur')
				rocket.animate({
					bottom:endBottom+'px'
				},400,function(){
					rocket.hide()
					 .css('bottom',startBottom+'px')

					
					return ;
					var tipsWidth=tips.width()

					tips.css('width','0').show()
					tips.animate({
						width:tipsWidth
					},400)
				})
			})
			smoke.show()
			smoke.animate({
				bottom:smokeBottom-15,
				right:smokeRight-25,

				width:smokeWidth+50,
				height:smokeHeight+20,
				opacity:0.6
			},600,function(){
				smoke.hide()
				smoke.css('bottom',smokeBottom+'px')
					.css('right',smokeRight+'px')
					.width('width',smokeWidth+'px')
					.height('height',smokeHeight+'px')
			})

			rw.hide()
		})

		shakeRocket(30,0,2,function(){
			setTimeout(function(){
				jumpyInterval=setInterval(jumpyRocket,40)
			},100)
			
		})
		
		
		var dy=0,step=1,direction=step,distance=5
		function jumpyRocket(){
			var s=rocket.attr('state')||''
			s=parseInt(s.substr(2))
			if(isNaN(s)){
				s=0
			}
			s++
			s=s%4
			rocket.attr('state','gf'+s)

			//上下抖
			if(dy>distance && direction==step){
				direction=-step
			}else if(dy<-distance && direction== -step){
				direction=step
			}
			dy+=direction
			rocket.css('bottom',(orignalBottom+dy)+'px')
		}

		var sR=parseInt(rocket.css('right')),sD=5
		function shakeRocket(time,pos,step,cb){
			
			if(time==0){
				rocket.css('right',(sR)+'px')
				cb()
				return 
			}
			if(pos<-sD || pos>sD){
				step=-step
			}
			pos+=step
			rocket.css('right',(sR+pos)+'px')

			setTimeout(function(){
				shakeRocket(time-1,pos,step,cb)
			},40)
		}


	}
	function run(){
		injectCss();
		// injectVideo();
		var day=7
		if(hasQd){
			day=5
		}
		day=3

		var cur=new Date().getTime()
		var expr=getCookie('gj_nav_expr')||0
		if(!expr || expr<cur ||TEST===true){
			setCookie('gj_nav_expr',cur+day*24*3600*1000,30);
			var inject=false
			$(window).bind('scroll',function(){
				if(!inject && $(this).scrollTop()>200 ){
					inject=true
					injectNav();
					// injectRocket();
				}
			})
		}else{

			if($('#scrollBtn').length>0){
				setTimeout(function(){
					// injectSlide();
				},1000)
			}
		}

	}
	setTimeout(function(){
		run()
	},1000)

}

