$(function(){

	var pvid=getCookie('gj_mpvid');
if(!pvid){
	pvid=parseInt(Math.random()*100000000)
	setCookie('gj_mpvid',pvid,7)
}
var gjt=getCookie('gj_t')||0;

function setCookie(name,value,expiredays){
	var exdate=new Date()
	exdate.setDate(exdate.getDate()+expiredays)
	document.cookie=name+ "=" +escape(value)+';path=/'+
	// document.cookie=name+ "=" +escape(value)+';domain=qq.com;path=/'+
	((expiredays==null) ? "" : ";expires="+exdate.toGMTString())
}
function getCookie(name){
	if (document.cookie.length<=0){
		return '';
	}
	var start=document.cookie.indexOf(name + "=")
	if (start!=-1){ 
		start=start + name.length+1 
		var end=document.cookie.indexOf(";",start)
		if (end==-1) end=document.cookie.length
		return unescape(document.cookie.substring(start,end))
	}
	return ''
}
function report(params){
	// 'guid','qq','engine','tag','id','url','ext'
	var api = "//p.guanjia.qq.com/bin/monitor/report.php?r="+Math.random();
	var uin=getCookie('uin')
	if(uin==''){
		uin=getCookie('o_cookie')
	}
	var dft={
		tag:'news.omg',
		uin:uin.replace(/^0*[^0-9]*0*/,''),
		m_pvid:getCookie('gj_mpvid'),
		domain:location.host
	}
	for(var d in dft){
		if(params[d]!=undefined)continue;
		params[d]=dft[d]
	}
	for(var prop in params){
		api += "&"+prop+"="+encodeURIComponent(params[prop])
	}
	
	var img=new Image()
	img.onload=function(){
		img=null
	}
	img.src=api
}
function injectCss(){
	var cssText='.gj-sclear,.gj-sword1{font-size:14px}.gj-nclose span,.gj-sclear,.gj-vclose{cursor:pointer}.gj-spop{display:none;background:#e8333f;width:348px;height:54px;position:absolute;top:0;right:0}.gj-sclear,.gj-sword{display:inline-block;position:absolute}li.gj-sbtn .gj-sa,li.gj-sbtn .gj-sa:hover{background:#c8333f;line-height:20px;text-align:center}.gj-sclear{background:#fff;color:#0b62c3;width:90px;text-align:center;line-height:30px;right:12px;top:12px}.gj-style-red .gj-sclear{color:#cf0022}.gj-style-gold .gj-sclear{color:#55462e;background:#ffde28}.gj-sword,.gj-v{text-align:left;color:#fff}.gj-sword{width:172px;height:30px;left:66px;top:10px}.gj-sicon,.gj-vclose{top:0;display:inline-block}.gj-sword2,.gj-v{font-size:12px}.gj-sicon{position:absolute;left:3px;width:54px;height:54px}.gj-v{height:32px;line-height:32px;padding-left:40px;position:relative}.gj-vclose{font-size:19px;position:absolute;right:8px}.gj-v a.gj-vbtn{color:#ffde28;text-decoration:underline;margin-left:5px;cursor:pointer}.gj-nav{position:fixed;right:21px;top:90px;font-size:14px;font-family:"微软雅黑"}.gj-nclose{text-align:right;color:#0b62c3;font-size:11px;text-decoration:underline}.gj-style-red .gj-nclose{color:#e8333f}.gj-style-gold .gj-nclose{color:#55462e}.gj-nb{position:relative;background:#379be9;width:358px;height:54px}.gj-nicon,.gj-nicon i,.gj-nword{position:absolute;display:inline-block}.gj-style-red .gj-nb{background:#e8333f}.gj-style-gold .gj-nb{background:#8d7a5d}.gj-nword{width:172px;height:30px;left:66px;top:10px;color:#fff;text-align:left;line-height:17px}.gj-nword1{color:#ffde28;font-size:14px;font-weight:700;line-height:17px}.gj-nword2{color:#fff;font-size:12px;margin-top:2px}.gj-nicon{left:-8px;top:-8px;width:68px;height:68px;background:url(data:image/png;base64,) no-repeat}.gj-nicon i{left:8px;top:8px;background:url(http://s.pc.qq.com/guanjia/gjalpha/omg/img/star-blue.gif) no-repeat;width:64px;height:54px}.gj-style-red .gj-nicon i{background:url(http://s.pc.qq.com/guanjia/gjalpha/omg/img/star.gif) no-repeat}.gj-style-gold .gj-nicon i{background:url(http://s.pc.qq.com/guanjia/gjalpha/omg/img/star-gold.gif) no-repeat}.gj-nav-run{display:none;position:fixed;right:21px;top:105px;line-height:54px;padding-left:64px;width:216px;box-sizing:content-box;background:url(data:image/gif;base64,) no-repeat #379be9;color:#fff;text-align:left;cursor:default}.gj-nav-run.gj-style-red{background:url(data:image/gif;base64,) no-repeat #ad160f}.gj-nav-run.gj-style-gold{background:url(data:image/gif;base64,) no-repeat #8D7A5D}';
	var style=document.createElement("style");
	style.setAttribute("type", "text/css");
	if (style.styleSheet) {   // for IE
		style.styleSheet.cssText = cssText;
	} else {// others
		var textnode = document.createTextNode(cssText);
		style.appendChild(textnode);
	}
	document.body.appendChild(style)
	
}
function getUrlArg(name) {
	var reg = new RegExp("(\\?|&)" + name + "=([^&]*)(&|$)","i");
	var r = window.location.href.match(reg)||[];
	var v=r[2];
	if(!v){return null}
	try{
		return decodeURIComponent(v);
	}catch(e){
		return v;
	}
}

function openDownload(){
	// window.open('http://dlied6.qq.com/invc/xfspeed/qqpcmgr/download/QQPCDownload78951.exe');
	window.open('https://pm.myapp.com/invc/xfspeed/qqpcmgr/download/QQPCDownload78879.exe');
	// window.open('http://guanjia.qq.com/product/clean/introduce/index.html');
};
	var hasQd=false,hasGj=false,hasJp=false;

	function allSetCookie(name,value,expiredays){
		var exdate=new Date()
		exdate.setDate(exdate.getDate()+expiredays)
		// document.cookie=name+ "=" +escape(value)+';path=/'+
		document.cookie=name+ "=" +escape(value)+';domain=qq.com;path=/'+
		((expiredays==null) ? "" : ";expires="+exdate.toGMTString())
	}
	// var TEST=true;
	var TEST=false;
	if(window.location.href.indexOf('debug=gj')>10){
		TEST=true;
	}

	var TK_NAME='_qpsvr_localtk'
	allSetCookie(TK_NAME,'tk'+parseInt(Math.random()*10000),1)

	function callQd(arg,portIndex){

		// return ;
		var port=[9410,16873]
		if(portIndex==undefined){
			portIndex=0
		}

		var df={
			_tk:getCookie(TK_NAME),
			timeout:5000,
			_ts:new Date().getTime()
		}
		df=$.extend(df,arg.data)


		var warg=[
		'_tk','_ts','timeout',
		'cmd','service','encrypt','action','lparam','wparam','session']
		var urlArg=''

		for (var i = 0; i < warg.length; i++) {
			var key=warg[i]
			urlArg+=key+'='+(df[key]||'')+'&'
		}
		
		$.ajax({
			data:{},
			dataType:"jsonp",
			type:'GET',
			jsonp:'callback',
			url:'https://localhost.sec.qq.com:'+port[portIndex]+'?'+urlArg,
			success:arg.success,
			timeout:3000,
			error:function(){},
			complete:function(jqXHR,textStatus){
				// "notmodified", "nocontent", "error", "timeout", "abort", "parsererror"
				if(textStatus=='success'){
					return ;
				}
				portIndex++
				if(portIndex>=port.length){
					arg.error()
					return;
				}

				callQd(arg,portIndex)
				// console.log('complete  '+textStatus)
			}
		})
	}

	if(TEST){
		runMain()
	}

	var expr=getCookie('gj_nav_expr')
	if(!expr || expr<new Date().getTime()){
		callQd({
			data:{
				cmd:101,
				service:1
			},
			success:function(json){
				hasQd=true
				//({qd: 1, guid: '5fee664522801d51e5d3d7599cb46c7b', osver: '6.1', checksum: '0', push: 0, qqpcmgr: 7, http_ver: 1});
				hasJp=true;
				if(json.http_ver>=2){
					hasJp=false;
					if((json.qqpcmgr&120)>0){
						hasJp=true
					}
				}
				if((json.qqpcmgr&1)==0){
					runMain()
					report({tag:'news.omg.nogj'})//无管家
					hasGj=false
				}else{
					report({tag:'news.omg.yesgj'})//有管家
					hasGj=true
				}

				if(hasJp){
					report({tag:'news.omg.yesjp'})
				}else{
					report({tag:'news.omg.nojp'})
				}
			},
			error:function(){
				report({tag:'news.omg.noqd'})
				// runMain()
				hasQd=false
				//如果出错三天不再发起请求
				setCookie('gj_nav_expr',new Date().getTime()+24*3600*1000)
			}
		})
	}else{
		report({tag:'news.omg.load.expr'})
	}
	report({tag:'news.omg.load'})//脚本跑起来

	

function runMain(){

	var tipsStyle={
		red:[
			'fashion.qq.com',
			'ent.qq.com'
		],
		gold:[
			'finance.qq.com',
			'money.qq.com',
			'stock.qq.com'
		]
	}

	function injectSlide(){
		var str=
		'<li class="gj-sbtn">'+
			'<a class="gj-sa" href="javascript:void(0);"><b>清理缓存</b></a>'+
			'<div class="gj-spop">'+
				'<span class="gj-sicon"></span>'+
				'<div class="gj-sword">'+
					'<div class="gj-sword1">看新闻，图片缓存多？</div>'+
					'<div class="gj-sword2">用电脑管家自动清理垃圾</div>'+
				'</div>'+
				'<span class="gj-sclear">安装并清理</span>'+
			'</div>'+
		'</li>'

		var btn=$('#scrollBtn')
		var home=btn.find('.sB-home')
		$(str).insertBefore(home)
		var pop=btn.find('.gj-spop')
		var box=btn.find('.gj-sbtn')
		var alink=box.find('.gj-sa')
		var icon=pop.find('.gj-sicon')
		var linkWidth=alink.width(),popWidth=pop.width()
		var clear=box.find('.gj-sclear')
		var word=box.find('.gj-sword')

		function slideShow(){
			pop.stop()
			pop.show();//alink.hide();
			clear.fadeOut(0)
			word.fadeOut(0)

			pop.animate({
				width:popWidth
			},200,function(){
				clear.fadeIn(100)
				word.fadeIn(100)
			})
		}
		function slideHide(){
			pop.stop()
			clear.hide()
			pop.animate({
				width:linkWidth
			},200,function(){
				pop.hide();//alink.show();
				clear.show()
			})
		}
		box.bind('mouseenter',function(){
			if(box.attr('fixed')=='1'){
				box.attr('fixed','0')
				return 
			}
			slideShow()
		})
		.bind('mouseleave',slideHide)

		var hoverReport=false
		clear.bind('click',function(){
			report({
				tag:'news.omg.article.install'
			})
			openDownload()
		}).bind('mouseenter',function(){
			if(hoverReport){
				return 
			}
			hoverReport=true
			report({
				tag:'news.omg.article.hover'
			})
		})
		
		
		report({
			tag:'news.omg.article.show'
		})
		//第一页并且时间24小时内，展示一次长条，并收缩动画
		var time=getCookie('gj_t')||0
		var cur=parseInt(new Date().getTime()/1000);
		if((cur-time)>86400 ){
		// if((cur-time)>10 ){
			setCookie('gj_t',cur,7);
			slideShow()
			box.attr('fixed','1')
			setTimeout(function(){
				slideHide()
			},5000)
		}else{
			pop.width(linkWidth)
		}
	}

	function injectVideo(){
