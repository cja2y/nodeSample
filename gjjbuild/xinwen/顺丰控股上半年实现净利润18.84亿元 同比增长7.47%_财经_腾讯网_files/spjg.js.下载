			return ;
		}
		video.append(str)
		var box=video.find('.gj-v')

		box.find('.gj-vclose').bind('click',function(){
			box.hide()
		})
		var hoverReport=false;
		box.find('.gj-vbtn').bind('click',function(){
			report({
				tag:'news.omg.video.install'
			})
			openDownload()
		}).bind('mouseenter',function(){
			if(hoverReport){
				return ;
			}
			hoverReport=true
			report({
				tag:'news.omg.video.hover'
			})
		})

		report({
			tag:'news.omg.video.show'
		})
		
	}

	function injectNav(){
		var link=location.href,style='blue';
		for(var s in tipsStyle){
			var list=tipsStyle[s]
			for (var i = 0; i < list.length; i++) {
				var pos=link.indexOf(list[i])
				if(pos>6&&pos<12){
					style=s
					break;
				}
			}
		}

		// style='red'
		var str=
		'<div class="gj-nav gj-style-'+style+'">'+
			'<div class="gj-nclose"><span>忽略提醒</span></div>'+
			'<div class="gj-nb">'+
				'<span class="gj-nicon"><i></i></span>'+
				'<div class="gj-nword">'+
					'<div class="gj-nword1">电脑卡？图片缓存太多！'+(TEST?'！':'')+'</div>'+
					'<div class="gj-nword2">用电脑管家自动清理垃圾</div>'+
				'</div>'+
				'<span class="gj-sclear">立即清理</span>'+
			'</div>'+
		'</div>'+
		'<div class="gj-nav-run gj-style-'+style+'">正在启动电脑管家清理模块...</div>'

		var wrap=$('body')
		if(wrap.length<=0){
			wrap=$('.nba-header').eq(0)
			if(wrap.length<=0){
				return ;
			}			
		}
		wrap.append(str)
		var box=wrap.find('.gj-nav')
		var runBox=wrap.find('.gj-nav-run')
		var close=box.find('.gj-nclose')

		close.bind('click',function(){
			box.hide()
		})
		var hoverReport=false
		box.find('.gj-sclear').click(function(){

			report({
				tag:'news.omg.nav.install',
				name:hasJp?'yesjp':'nojp'
			})
			var paramOld='K3GihyfS25kxuOnLqsP42c48biBCp+SeDXdyeA7Tg53jUiDYv+ZS1JfHUth7bsORxeNGwQ3Sh8TlSG5yBltfhDFDsqasJLtlqLe73HNjhGwCmZg5pn8NUT/PwS787ul3S/zrRW0ODDTvtay7of8mTT0QeDZgS2Tp1fw4c9eb7wY=';
			var paramNew='eTeYVxBSSsqcYkoqU5njVi8X5VcrVA8aeFm4q2/PtC50Ls/EMoL5/AUZhzz6ihDNWRCfVGQ8BGhXPwR/wtDXp/8mGYa2PAOqt4UW7VQoxB/Vq9M59u5DyPd8rqFb0dnK8OsBkm0reTEYwP6RekDYSwjTpYc8BLDwJT2LF21UQQ8=';
			var params=paramOld;
			
			if(hasQd && (!hasGj) &&hasJp){
				//有q盾无管家
				open('https://pm.myapp.com/invc/xfspeed/qqpcmgr/download/QQPCDownload79212.exe');
				params=paramOld;
			}
			if(hasQd && (!hasGj) && (!hasJp)){
				params=paramNew;
			}
			
			box.animate({
				right:'-400px'
			},300,function(){
				runBox.show().css('right','-280px').animate({
					right:'21px'
